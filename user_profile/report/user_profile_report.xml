<odoo>
    <data>
        <report file="user_profile.report_profile" id="report_profile" model="user.profile" name="user_profile.report_user_profile" report_type="qweb-pdf" string="Profile Report"/>
        <template id="report_user_profile">
            <t t-call="report.html_container">
                <t t-as="doc" t-foreach="docs">
                    <t t-call="report.external_layout">
                        <div class="page">
                            <h2 t-field="doc.name"/>
                            <p><span t-field="doc.gender"/></p>
                            <p><span t-field="doc.birthday"/></p>
                            <p><span t-field="doc.age"/></p>
                            <p><span t-field="doc.nickname_ids"/></p>
                            <p><h2>Hobbies</h2></p>
                            <table class="table table-bordered">
                                <tr>
                                   <th>Name</th> 
                                   <th>Dedicated Hours</th>
                                </tr>
                                <t t-foreach="doc.hobbie_ids" t-as="hobbie"> 
                                   <tr>
                                       <td>name</td>
                                       <td>dedocated_hours</td>
                                   </tr>
                                </t>
                            </table>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>